{
    "name": "Pokemon Showdown Enhanced Tooptip",
    "description": "Enhanced tooltip for Pokemon Showdown - shows damage calcs, type effectiveness, and more",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts":[
        {
        "matches": ["https://play.pokemonshowdown.com/*"],
        "js": ["enhanced-tooltip.js"]
        }
    ],
    "icons": {
        "16": "/images/icons/pokeball16.png",
        "32": "/images/icons/pokeball32.png",
        "48": "/images/icons/pokeball48.png",
        "128": "/images/icons/pokeball128.png"
    }
  }